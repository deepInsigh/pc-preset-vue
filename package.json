{
  "name": "pc-template",
  "private": true,
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 6"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "pnpm bootstrap && vite",
    "build": "export env TZ='Asia/Shanghai' && vite build",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@icon-park/vue-next": "1.4.2",
    "@quantum-asia/qt-design": "1.5.41",
    "@vueuse/core": "10.7.0",
    "axios": "1.6.2",
    "crypto-js": "4.2.0",
    "date-fns": "2.30.0",
    "idb": "8.0.0",
    "lodash-es": "4.17.21",
    "naive-ui": "2.36.0",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "3.2.1",
    "vue": "3.3.13",
    "vue-i18n": "9.8.0",
    "vue-router": "4.2.5",
    "vxe-table": "4.5.15",
    "xe-utils": "3.5.14"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@quantum-asia/vite-plugin-log-info": "1.0.1",
    "@quantum-asia/vite-plugin-vue-components": "1.0.32",
    "@types/node": "20.10.5",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "@vitejs/plugin-vue": "4.5.2",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "autoprefixer": "10.4.16",
    "commitizen": "4.3.0",
    "commitlint-config-cz": "0.13.3",
    "cross-env": "7.0.3",
    "cz-customizable": "7.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.0",
    "eslint-plugin-vue": "9.19.2",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "postcss": "8.4.32",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.1.1",
    "sass": "1.69.5",
    "stylelint": "16.0.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "35.0.0",
    "ts-node": "10.9.2",
    "type-fest": "4.8.3",
    "typescript": "5.3.3",
    "unocss": "0.58.0",
    "unplugin-auto-import": "0.17.2",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.0.10",
    "vite-plugin-top-level-await": "1.4.1",
    "vite-plugin-windicss": "1.9.2",
    "vue-eslint-parser": "9.3.2",
    "vue-tsc": "1.8.25"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{vue,scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%, last 1 version"
  ]
}
