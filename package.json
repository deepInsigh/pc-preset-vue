{
  "name": "pc-template",
  "private": true,
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 6"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "pnpm bootstrap && vite",
    "build": "export env TZ='Asia/Shanghai' && vite build",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@icon-park/vue-next": "1.4.2",
    "@quantum-asia/qt-design": "1.4.13",
    "@vueuse/core": "10.2.1",
    "axios": "1.4.0",
    "crypto-js": "4.1.1",
    "date-fns": "2.30.0",
    "idb": "7.1.1",
    "lodash-es": "4.17.21",
    "naive-ui": "2.34.4",
    "pinia": "2.1.4",
    "pinia-plugin-persistedstate": "3.1.0",
    "vue": "3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.4",
    "vxe-table": "4.4.2",
    "xe-utils": "3.5.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@quantum-asia/vite-plugin-log-info": "1.0.2",
    "@quantum-asia/vite-plugin-vue-components": "1.0.10",
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "autoprefixer": "10.4.14",
    "commitizen": "4.3.0",
    "commitlint-config-cz": "0.13.3",
    "cross-env": "7.0.3",
    "cz-customizable": "7.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.15.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.26",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "3.0.0",
    "sass": "1.63.6",
    "stylelint": "15.10.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "ts-node": "10.9.1",
    "type-fest": "3.13.0",
    "typescript": "5.1.6",
    "unocss": "0.53.5",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.1",
    "vite": "4.4.4",
    "vite-plugin-top-level-await": "1.3.1",
    "vite-plugin-windicss": "1.9.0",
    "vue-eslint-parser": "9.3.1",
    "vue-tsc": "1.8.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{vue,scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%, last 1 version"
  ]
}
